{
    "manifest_version": 3,
    "name": "QuickLike for YouTube",
    "version": "1.0.0",
    "description": "Adds convenient like/dislike buttons to YouTube's video player controls for quick interactions without leaving fullscreen.",
    "author": "Your Name",
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "permissions": ["activeTab"],
    "content_scripts": [
        {
            "matches": ["*://*.youtube.com/*"],
            "css": ["styles.css"],
            "js": ["content.js"]
        }
    ],
    "homepage_url": "https://github.com/yourusername/quicklike"
}
